{
  "expo": {
    "name": "ColorHarmonizer",
    "slug": "ColorHarmonizer",
    "version": "2.0.2",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/appIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.clarkbarrett101.ColorHarmonizer",
      "buildNumber": "2.0.2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.clarkbarrett101.ColorHarmonizer"
    },
    "web": {
      "favicon": "./assets/appIcon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6f05c862-6a0c-4d89-809d-82f955b7e394"
      }
    },
    "plugins": [
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "ColorHarmonizer needs access to your camera so you can measure undertones and view walls with different paint colors."
        }
      ]
    ]
  }
}
